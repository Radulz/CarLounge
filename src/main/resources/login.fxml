<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<StackPane fx:id="mainContainer" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.CarLounge.fis.controller.LogInController">
   <children>
      <AnchorPane fx:id="logInContainer" prefHeight="400.0" prefWidth="360.0">
         <children>
            <Label alignment="CENTER" layoutX="117.0" layoutY="20.0" text="CarLounge" textAlignment="CENTER">
               <font>
                  <Font name="System Bold" size="25.0" />
               </font>
            </Label>
            <Label alignment="CENTER" layoutX="58.0" layoutY="113.0" text="Username">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <Label alignment="CENTER" layoutX="61.0" layoutY="163.0" text="Password">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <TextField fx:id="userField" layoutX="155.0" layoutY="113.0" />
            <PasswordField fx:id="passwordField" layoutX="155.0" layoutY="163.0" />
            <Button fx:id="logInToAcc" layoutX="144.0" layoutY="222.0" mnemonicParsing="false" onMouseClicked="#switchToHomeScene" prefHeight="25.0" prefWidth="72.0" text="Log in" />
            <Button fx:id="goToChoice" layoutX="144.0" layoutY="270.0" mnemonicParsing="false" onMouseClicked="#switchToChoice" prefHeight="25.0" prefWidth="72.0" text="Register" />
         </children>
      </AnchorPane>
   </children>
</StackPane>
